<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.xottys.userinterface.animation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TweenedAnimationActivity"
            android:label="TweenedAnimation"/>
        <activity android:name=".FrameAnimationActivity"
            android:label="FrameAnimation" />
        <activity android:name=".BasicPropertyAnimationActivity"
            android:label="PropertyAnimation1"/>
        <activity android:name=".PropertyAnimationActivity2"
            android:label="PropertyAnimation2"/>
        <activity android:name=".ViewAnimatorActivity"
            android:label="ViewAnimator"/>
        <activity android:name=".PhysicsAnimationActivity"
            android:label="PhysicsAnimation"/>
        <activity android:name=".TransitionAnimationActivity"
            android:label="TransitionAnimation"/>
        <activity android:name=".CustomSVGActivity"
            android:label="Custom SVG Animation"/>
        <activity android:name=".MiscAnimationActivity"/>
        <activity android:name=".ThreeDimensionAnimationActivity"/>
    </application>

</manifest>