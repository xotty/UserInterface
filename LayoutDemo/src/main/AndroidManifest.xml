<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.xottys.userinterface">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LinearLayoutActivity"
            android:label="LinearLayout">
        </activity>
        <activity android:name=".TableLayoutActivity"
            android:label="TableLayout">
        </activity>
        <activity android:name=".RelativeLayoutActivity"
            android:label="RelativeLayout">
        </activity>
        <activity android:name=".GridLayoutActivity"
            android:label="GridLayout">
        </activity>
        <activity android:name=".FrameLayoutActivity"
            android:label="FrameLayout">
        </activity>
        <activity
            android:name=".ConstraintLayoutActivity"
            android:label="ConstraintLayout"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        </activity>
    </application>
</manifest>
